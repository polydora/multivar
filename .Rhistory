for ( i in 1:n) {
for (j in i:n) {
if (dist[i, j] < = Dcrit) {
pl <- pl + geom_segment(x = points$X[i], y = points$Y[i], xend = points$X[j], yend = points$Y[j])
}
}
}
pl
}
