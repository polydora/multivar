plejada <- function (data, method = "bray", Dcrit) {
# n - число точек
# p - период
require(ggplot2)
n <- nrow(data)
p <- n
factor<- points <- data.frame(X = c(1:n), Y = c(1:n))
k <- 0
for (i in 1:n){
factor$X[i] <- (i-1)/p - k
if ((i/p - trunc(i/p)) == 0) k <- k + 1
}
factor$Y <- factor$X
for (i in 1:n){
points$X[i] <- cos(2*pi*factor$X[i])
points$Y[i] <- sin(2*pi*factor$Y[i])
}
lab <- as.character(1:n)
pl <- ggplot(points, aes(x = X, y = Y)) + geom_text(label = lab, size = 10, ) + theme(panel.grid.major = element_blank(), panel.grid.minor = element_blank(), panel.background = element_rect(fill = "white"), axis.ticks.x = element_blank(), axis.ticks.y = element_blank(), axis.text = element_blank()) + xlab("") + ylab("")
require(vegan)
dist <- as.matrix(vegdist(data, method = method))
for ( i in 1:n) for (j in i:n) if (dist[i, j] < = Dcrit) pl <- pl + geom_segment(x = points$X[i], y = points$Y[i], xend = points$X[j], yend = points$Y[j])
pl
}
