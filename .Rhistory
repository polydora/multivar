# n - число точек
# p - период
library(ggplot2)
p <- n <- nrow(data)
factor <- points <- data.frame(X = c(1:n), Y = c(1:n))
k <- 0
for (i in 1:n){
factor$X[i] <- (i-1)/p - k
if ((i/p - trunc(i/p)) == 0) {
k <- k + 1
}
}
factor$Y <- factor$X
for (i in 1:n){
points$X[i] <- cos(2*pi*factor$X[i])
points$Y[i] <- sin(2*pi*factor$Y[i])
}
lab <- as.character(1:n)
pl <- ggplot(points, aes(x = X, y = Y)) + geom_text(label = lab, size = 10, ) + theme_minimal()
